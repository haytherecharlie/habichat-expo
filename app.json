{
  "expo": {
    "name": "habichat-app",
    "slug": "habichat-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./static/icon.png",
    "splash": {
      "image": "./static/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./static/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./static/favicon.png"
    }
  }
}
